<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myannonces">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Myannonces">
        <activity
            android:name="com.example.myannonces.ui.activities.DashboardActivity"
            android:label="@string/app_name"
            />

        <activity
            android:name="com.example.myannonces.ui.activities.SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Myannonces"

            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.myannonces.ui.activities.MainActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.example.myannonces.ui.activities.LoginActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Myannonces" />

        <activity
            android:name="com.example.myannonces.ui.activities.RegisterActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Myannonces" />

        <activity android:name="com.example.myannonces.ui.activities.ForgotPasswordActivity" />

        <activity
            android:name="com.example.myannonces.ui.activities.UserProfileActivity"
            android:screenOrientation="portrait" />

        <activity android:name="com.example.myannonces.ui.activities.BaseActivity" />




    </application>

</manifest>